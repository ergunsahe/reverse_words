    "\n",
    "def reverse_words(user_input):\n",
    "    input_list = user_input.split(\" \")\n",
    "    return \" \".join(input_list[::-1])\n",
    "user_input = input(\"Please write to something you want to reverse: \")\n",
    "reverse_words(user_input)"
